<?xml version="1.0" encoding="utf-8"?>
<FormalDescription Name="AiCAi" Description="A 2-k turing machine that recognizes the language given by L={a^ica^i}" NumberOfTapes="2" BlankSymbol="B" LeftBounded="False">
